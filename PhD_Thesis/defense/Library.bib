	Issn = {0027-8424},
	Journal = {Proceedings of the National Academy of Sciences},
	Keywords = {Polariton},
	Number = {13},
	Pages = {E1516--E1519},
	Publisher = {National Academy of Sciences},
	Title = {Weak lasing in one-dimensional polariton superlattices},
