	Issn = {0027-8424},
	Journal = {Proc. Natl. Acad. Sci. U. S. A.},
	Keywords = {Polariton},
	Number = {13},
	Pages = {E1516--E1519},
	Publisher = {National Academy of Sciences},
	Title = {Weak lasing in one-dimensional polariton superlattices},
